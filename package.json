{
  "name": "autos-s4e",
  "version": "0.0.1494",
  "description": "prototype - a simple GraphQL demo rendered with Svelte and Tailwind",
  "author": "eswat2",
  "license": "MIT",
  "repository": "eswat2/autos-s4e",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "bump": "bumpp patch --commit --tag --push --yes",
    "deploy-app": "cd dist; ln -s ../.vercel; vc --prod",
    "release": "pnpm bump && pnpm build && pnpm deploy-app",
    "preinstall": "npx only-allow pnpm",
    "dev": "concurrently \"pnpm:twx --watch\" \"pnpm:start\"",
    "twx": "sh ./.generate",
    "start": "vite",
    "prebuild": "echo skipping pnpm format",
    "build": "pnpm twx; vite build",
    "serve": "vite preview",
    "cc": "prettier --check './src/**/*.{css,pcss,js,ts,svelte}'",
    "format": "prettier --write './src/**/*.{css,pcss,js,ts,svelte}'",
    "spell": "cspell './**/*.js'",
    "open": "open https://autos-s4e.vercel.app"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "6.2.4",
    "autoprefixer": "10.4.24",
    "bumpp": "10.4.0",
    "concurrently": "9.2.1",
    "fetchql": "3.0.0",
    "only-allow": "1.2.2",
    "postcss": "8.5.6",
    "prettier": "3.8.1",
    "prettier-plugin-svelte": "3.4.1",
    "prettier-plugin-tailwindcss": "0.7.2",
    "proto-tailwindcss-clrs": "0.0.464",
    "svelte": "5.49.1",
    "tailwindcss": "4.1.18",
    "vite": "7.3.1",
    "yoctocolors": "2.1.2"
  }
}
