{
  "name": "autos-s4e",
  "version": "0.0.1469",
  "description": "prototype - a simple GraphQL demo rendered with Svelte and Tailwind",
  "author": "eswat2",
  "license": "MIT",
  "repository": "eswat2/autos-s4e",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "concurrently \"yarn:twx --watch\" \"yarn:start\"",
    "twx": "sh ./.generate",
    "start": "vite",
    "prebuild": "echo skipping yarn format",
    "build": "yarn twx; vite build",
    "serve": "vite preview",
    "cc": "prettier --check './src/**/*.{css,pcss,js,ts,svelte}'",
    "format": "prettier --write './src/**/*.{css,pcss,js,ts,svelte}'",
    "spell": "cspell './**/*.js'",
    "predeploy": "yarn version --patch; yarn build",
    "deploy": "cd dist; ln -s ../.vercel; vc --prod",
    "open": "open https://autos-s4e.vercel.app"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "6.2.4",
    "autoprefixer": "10.4.23",
    "concurrently": "9.2.1",
    "fetchql": "3.0.0",
    "postcss": "8.5.6",
    "prettier": "3.7.4",
    "prettier-plugin-svelte": "3.4.1",
    "prettier-plugin-tailwindcss": "0.7.2",
    "proto-tailwindcss-clrs": "0.0.458",
    "svelte": "5.46.1",
    "tailwindcss": "4.1.18",
    "vite": "7.3.1"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
