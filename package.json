{
  "name": "autos-s4e",
  "version": "0.0.74",
  "description": "prototype - a simple GraphQL demo rendered with Svelte and Tailwind",
  "author": "eswat2",
  "license": "MIT",
  "repository": "eswat2/autos-s4e",
  "type": "module",
  "scripts": {
    "twx": "sh .tw-version",
    "dev": "yarn twx; vite",
    "build": "yarn twx; yarn format; vite build",
    "serve": "vite preview",
    "spell": "cspell *.{html,json,js,md,ts} src/*.{css,ts,js,svelte}",
    "cc": "prettier --plugin-search-dir=. --check src",
    "format": "prettier --plugin-search-dir=. --write src",
    "predeploy": "yarn version --patch; yarn build",
    "deploy": "cd dist; ln -s ../.vercel; vc --prod",
    "open": "open https://autos-s4e.vercel.app"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "1.0.0-next.33",
    "autoprefixer": "10.4.1",
    "cssnano": "5.0.14",
    "fetchql": "3.0.0",
    "postcss": "8.4.5",
    "postcss-load-config": "3.1.1",
    "prettier": "2.5.1",
    "prettier-plugin-svelte": "2.5.1",
    "proto-tailwindcss-clrs": "0.0.84",
    "source-map-js": "1.0.1",
    "svelte": "3.44.3",
    "svelte-preprocess": "4.10.1",
    "tailwindcss": "3.0.9",
    "vite": "2.7.10"
  }
}
